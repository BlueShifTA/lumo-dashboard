{
  "name": "frontend",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000",
    "build": "next build",
    "typecheck": "tsc --noEmit",
    "start": "next start",
    "lint": "eslint .",
    "update-api-schema": "wget -q http://127.0.0.1:8002/openapi.json -O openapi.json && prettier --write ./openapi.json",
    "create-api-types": "orval --config orval.config.ts",
    "api": "npm run update-api-schema && npm run create-api-types"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.1.0",
    "@tanstack/react-query": "^5.76.1",
    "axios": "^1.9.0",
    "next": "^15.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
    "@types/axios": "^0.14.4",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "eslint": "^9.0.0",
    "eslint-config-next": "^15.0.0",
    "orval": "^7.9.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  }
}
